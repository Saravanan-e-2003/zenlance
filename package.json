{
  "name": "vibe-code-fullstack",
  "version": "1.0.0",
  "description": "FreelanceHub - Full-stack business management application",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd forntend && npm run dev",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd forntend && npm run build && npm run preview",
    "install:all": "npm install && cd backend && npm install && cd ../forntend && npm install",
    "build": "cd forntend && npm run build",
    "test": "cd backend && npm test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "freelance",
    "business-management",
    "crm",
    "invoicing"
  ],
  "author": "Your Name",
  "license": "MIT"
}
